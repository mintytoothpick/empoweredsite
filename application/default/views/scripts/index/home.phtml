<script type="text/javascript">
    var current_bubble = 1;
    function showVolunteers(div, link) {
        if (div.search('all') > -1) {
            $(div.replace('all', 'less')).hide();
            $(link.replace('Less', 'More')).hide();
        } else {
            $(div.replace('less', 'all')).hide();
            $(link.replace('More', 'Less')).hide();
        }
        $(div).show();
        $(link).show();
    }

    $(function() {
        $('.bullets span').bind('click', function() {
            var id = $(this).attr('id');
            $('.bullets span').each(function() {
                $(this).removeClass('active');
            })
            $(this).addClass('active');
            $('.bubble-quote').each(function() {
                $(this).hide();
            })
            $('.quoted-by').each(function() {
                $(this).hide();
            })
            $('.quote-'+id).show();
            current_bubble = Number(id);
        })
    })
    function next() {
	if(current_bubble < 3) {
            current_bubble++;
            $('#'+current_bubble).click();
	}
    }
    function prev() {
	if(current_bubble > 1) {
            current_bubble--;
            $('#'+current_bubble).click();
	}
    }

