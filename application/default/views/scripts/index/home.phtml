<script>
    var next = 1;
    var intervalID;
    $(function() {
        intervalID = window.setInterval(autoPlay, 4000);
    })
    var current_playing = 1;
    function autoPlay() {
        $('.slideshow > span').removeClass('active');
        $('#button-'+current_playing).addClass('active');
        $('#slide-holder').attr('class', 'slide-'+current_playing);
        $('.text-messages > span').hide();
        $('#slide-'+current_playing).show();
        $('#text-'+current_playing).show();
        $('.new-banner').css('color', '#000');
        $('#new-banner-'+current_playing).css('color', '#693');
        if (current_playing < 5) {
            current_playing++;
        } else {
            current_playing = 1;
        }
    }
    $(function() {
        $('.slide-buttons').bind('click', function() {
            $('.slideshow > span').removeClass('active');
            $(this).addClass('active');
            var id = $(this).attr('id').replace('button-', '');
            $('#slide-holder').attr('class', 'slide-'+id);
            $('.text-messages > span').hide();
            $('#slide-'+id).show();
            $('#text-'+id).show();
            $('.new-banner').css('color', '#000');
            $('#new-banner-'+id).css('color', '#693');
            /*
            if (Number(id) > 1) {
                $('.prev-slide').show();
            } else {
                $('.prev-slide').hide();
            }
            if (Number(id) < 5) {
                $('.next-slide').show();
            } else {
                $('.next-slide').hide();
            }
            */
           if (intervalID != null) {
               clearInterval(intervalID);
           }
        })
    })
    
    $(function() {
        $('#slide-holder').bind('click', function() {
            var id = $(this).attr('class').replace('slide-', '');
            if(id == 1) {
                var top = (screen.height/2)-274;
                $('.popup-box').toggle().css('top', top);
                $('#popup-overlay').toggle();
            }
        })
    })
    
    function nextSlide() {
        if (next < 5) {
            var active = $('.slideshow > span.active').attr('id').replace('button-', '');
            $('#button-'+(Number(active) + 1)).click()
            next++;
        } else {
            //clearInterval(intervalID);
            $('#button-1').click()
            next = 1;
        }
    }
    
    function prevSlide() {
        if (next < 5) {
            var active = $('.slideshow > span.active').attr('id').replace('button-', '');
            $('#button-'+(Number(active) - 1)).click()
            next--;
        } else {
            $('#button-5').click()
            next = 5;
        }
    }
</script>
