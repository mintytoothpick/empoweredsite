<script>
    function closeVideo() {
        $('#popup-overlay').hide();
        $('.popup-box').hide();

        var video = document.getElementsByTagName("video")[0]
        if( video.pause ) {
            video.pause();
        }
    }

    function sendForm() {
        $.fancybox.showActivity();

        $.ajax({
            url: '/tour/send-apply-form',
            type: 'POST',
            data: $('#fancybox-content .frmApply').serialize(),
            success: function() {
                $.fancybox.hideActivity();
                $.fancybox.close();
            }
        });
    }

    $(function() {
        $('#slide-holder').bind('click', function() {
            var id = $(this).attr('class').replace('slide-', '');
            if(id == 1) {
                var top = (screen.height/2)-274;
                $('.popup-box').toggle().css('top', top);
                $('#popup-overlay').toggle();

                var video = document.getElementsByTagName("video")[0]
                if( video.play ) {
                    video.play();
                }
            }
        });

    })
</script>

